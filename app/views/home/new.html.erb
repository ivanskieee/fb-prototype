<h1>Create a account for Facebook</h1>

<%= form_with model: @homes do |form| %>
  <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.label :password %><br>
    <%= form.text_field :password %><br>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
